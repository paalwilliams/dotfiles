[
  // Additional VS Code bindings...
  {
    "context": "Editor",
    "bindings": {
      "alt-up": "editor::MoveLineUp",
      "alt-down": "editor::MoveLineDown",
      "alt-shift-down": "editor::DuplicateLine",
      "cmd-shift-v": "workspace::ToggleVimMode",
      "alt-escape": "editor::ShowCompletions",
      "cmd-ctrl-shift-alt-j": "pane::ActivatePrevItem",
      "cmd-ctrl-shift-alt-k": "pane::ActivateNextItem"
    }
  }
]
